{
  "version": 2,
  "builds": [
    {
      "src": "./app.js",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "PORT": "3000",
    "FAKESTORE": "https://fakestoreapi.com"
  }
}